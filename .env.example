# Gemini API Key (코드 리뷰용 - 필수)
GEMINI_API_KEY=your_gemini_api_key_here

# GitHub 설정 (선택사항)
GITHUB_TOKEN=github_token_here  # Actions에서는 자동 제공
GITHUB_OWNER=your_github_username
GITHUB_REPO=n8n-24workflow

# 로컬 머신 설정
MACHINE_NAME=local-mac  # 본인 컴퓨터 이름

# 실행 설정
CHECK_INTERVAL=30  # 작업 확인 주기 (분)
AUTO_COMMIT=true   # 자동 커밋 여부
AUTO_PR=true       # 자동 PR 생성 여부